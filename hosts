[master]
# 如果部署单Master，只保留一个Master节点
# 默认Naster节点也部署Node组件
192.168.0.11 node_name=k8s-master1-x1
192.168.0.12 node_name=k8s-master2-x2
192.168.0.13 node_name=k8s-master3-x3

[node]
172.18.27.13 node_name=k8s-node1-lb3

[etcd]
192.168.0.11 etcd_name=etcd-1
192.168.0.12 etcd_name=etcd-2
192.168.0.13 etcd_name=etcd-3

[lb]
# 如果部署单Master，该项忽略
172.18.27.11 lb_name=lb-master
172.18.27.12 lb_name=lb-backup

[k8s:children]
